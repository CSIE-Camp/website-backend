{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": ["error", { "code": 100 }],
        "quotes": ["error", "double"],
        "semi": ["error", "always"],
        "indent": ["error", "tab"],
        "comma-dangle": ["error", "always-multiline"],
    }
}
